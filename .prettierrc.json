{
	"printWidth": 80,
	"useTabs": true,
	"tabWidth": 2,
	"singleQuote": true,
	"quoteProps": "as-needed",
	"jsxSingleQuote": true,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"arrowParens": "always",
	"semi": true,
	"bracketSameLine": true,
	"htmlWhitespaceSensitivity": "css",
	"endOfLine": "lf",
	"vueIndentScriptAndStyle": true,
	"singleAttributePerLine": true
}
